n=int(input())
arr=[]
locations = []
i=0
for i in range(i,n):
    a=int(input())
    arr.append(a)
f=int(input())
occurrences = 0
for i, a in enumerate(arr, start=1):
    if a == f:
        locations.append(i)
        occurrences += 1
for location in locations:
    print(f"{f} is present at location {location}.")
if(occurrences!=0):
    print(f"{f} is present {(occurrences)} times in the array.")
else:
    print(f"{f} is not present in the array.")
    
